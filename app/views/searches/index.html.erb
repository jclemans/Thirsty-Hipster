<div class="row">
  <div class="col-md-6 col-md-offset-1">
    <h1>Results</h1>

    <% locations = Search.yelp_check %>
    <% locations.each do |location| %>
    <h4><%= location.name %></h4>
    <%= location.address %>
    <% end %>
  </div>

  <div class="col-md-4">
    <div class="container">
      <h1>New Search</h1>
      <%= render 'form' %>
    </div>
  </div>



